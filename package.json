    "@langchain/openai": "^0.5.13",
    "@verax-attestation-registry/verax-sdk": "2.4.0",
    "dotenv": "^16.4.1",
    "telegraf": "^4.15.3",
    "viem": "^2.31.3",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.0",
