{
  "name": "langchain-agent",
  "version": "1.0.0",
  "description": "Platforme agentic modulaire avec support multi-fonctionnalit√©s",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "node --loader ts-node/esm src/index.ts",
    "test": "jest",
    "mcp:simple": "node dist/examples/mcp_simple_example.js",
    "mcp:agent": "node dist/examples/mcp_agent_example.js",
    "mcp:multi": "node dist/examples/mcp_multi_server_example.js",
